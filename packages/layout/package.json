{
  "name": "@logicflow/layout",
  "version": "1.2.0-alpha.17",
  "description": "LogicFlow layout",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "unpkg": "dist/index.min.js",
  "jsdelivr": "dist/index.min.js",
  "homepage": "https://site.logic-flow.cn",
  "repository": {
    "type": "git",
    "url": "https://github.com/didi/LogicFlow",
    "directory": "packages/layout"
  },
  "scripts": {
    "clean:turbo": "rss",
    "clean:build": "rss",
    "clean": "rss",
    "build:less": "rss",
    "build:esm": "tsc --module esnext --target es5 --outDir ./es",
    "build:cjs": "tsc --module commonjs --target es5 --outDir ./lib",
    "build:umd": "rollup -c ./rollup.config.js --bundleConfigAsCjs",
    "build:dev": "run-p -s build:cjs build:esm",
    "build:watch": "rss",
    "build:watch:esm": "rss",
    "build:watch:cjs": "rss",
    "build": "run-p -s build:dev build:umd",
    "prebuild": "rss",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "@logicflow/layout",
    "LogicFlow",
    "layout",
    "flowchart",
    "diagram"
  ],
  "author": "Logicflow-Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@logicflow/core": "workspace:*",
    "dagre": "^0.8.5"
  },
  "devDependencies": {
    "@types/dagre": "^0.7.53"
  }
}

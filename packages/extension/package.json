{
  "name": "@logicflow/extension",
  "version": "2.0.15",
  "description": "LogicFlow Extensions",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "unpkg": "dist/index.min.js",
  "jsdelivr": "dist/index.min.js",
  "homepage": "https://site.logic-flow.cn",
  "repository": {
    "type": "git",
    "url": "https://github.com/didi/LogicFlow",
    "directory": "packages/extension"
  },
  "scripts": {
    "clean:turbo": "rss",
    "clean:build": "rss",
    "clean": "rss",
    "build:less": "rss",
    "build:esm": "tsc --module esnext --target es5 --outDir ./es",
    "build:cjs": "tsc --module commonjs --target es5 --outDir ./lib",
    "build:umd": "rollup -c ./rollup.config.js --bundleConfigAsCjs",
    "build:dev": "run-p -s build:cjs build:esm",
    "build:watch": "rss",
    "build:watch:esm": "rss",
    "build:watch:cjs": "rss",
    "build": "run-p -s build:dev build:umd",
    "prebuild": "rss",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "@logicflow/extension",
    "LogicFlow"
  ],
  "author": "Logicflow-Team",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@logicflow/core": "workspace:*"
  },
  "dependencies": {
    "@antv/hierarchy": "^0.6.11",
    "@logicflow/core": "workspace:*",
    "classnames": "^2.3.2",
    "lodash-es": "^4.17.21",
    "medium-editor": "^5.23.3",
    "mobx": "^5.15.7",
    "preact": "^10.17.1",
    "rangy": "^1.3.1",
    "vanilla-picker": "^2.12.3"
  },
  "devDependencies": {
    "less": "^4.1.1",
    "postcss-url": "^10.1.3",
    "postcss-import": "^16.1.0",
    "rollup-plugin-postcss": "^4.0.2"
  }
}
